  <ripe_class name="mntner" main_sql_table="mntner" code="mt">
    <description>
      Objects in the RIPE Database may be protected using mntner
      (pronounced "maintainer") objects.  A mntner object specifies
      authentication information required to authorise creation,
      deletion or modification of the objects protected by the mntner.
      Mntner objects are not created automatically, but are forwarded
      to RIPE Database Administration (ripe-dbm@ripe.net).
    </description>
    <attributes>
      <mntner       choice="mandatory" number="single"   />
      <descr        choice="mandatory" number="multiple" />
      <org          choice="optional"  number="multiple" />
      <admin-c      choice="mandatory" number="multiple" />
      <tech-c       choice="optional"  number="multiple" />
      <upd-to       choice="mandatory" number="multiple" />
      <mnt-nfy      choice="optional"  number="multiple" />
      <auth         choice="mandatory" number="multiple" />
      <remarks      choice="optional"  number="multiple" />
      <notify       choice="optional"  number="multiple" />
      <abuse-mailbox choice="optional" number="multiple" />
      <mnt-by       choice="mandatory" number="multiple" />
      <referral-by  choice="mandatory" number="single"   />
      <changed      choice="mandatory" number="multiple" />
      <source       choice="mandatory" number="single"   />
      <delete       choice="optional"  number="single"   pseudo="yes"/>
      <override     choice="optional"  number="single"   pseudo="yes"/>
    </attributes>
    <dbase_code value="9"/>
    <rollback-tables>
      <table name="mntner"/>
      <table name="person_role"/>
      <table name="names"/>
      <table name="organisation"/>
    </rollback-tables>
    <affected-tables>
