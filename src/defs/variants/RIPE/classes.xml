  <ripe_class name="filter-set" main_sql_table="filter_set" code="fs">
    <description>
       A filter-set object defines a set of routes that are matched by
       its filter.  The "filter-set:" attribute defines the name of
       the filter.  It is an RPSL name that starts with "fltr-".  The
       "filter:" attribute defines the set's policy filter.   A policy
       filter is a logical expression which when applied to a set of
       routes returns a subset of these routes.
    </description>
    <attributes>
      <filter-set  choice="mandatory" number="single"   />
      <descr       choice="mandatory" number="multiple" />
      <filter      choice="optional"  number="single"   />
      <mp-filter   choice="optional"  number="single"   />
      <remarks     choice="optional"  number="multiple" />
      <org         choice="optional"  number="multiple" />
      <tech-c      choice="mandatory" number="multiple" />
      <admin-c     choice="mandatory" number="multiple" />
      <notify      choice="optional"  number="multiple" />
      <mnt-by      choice="mandatory" number="multiple" />
      <mnt-lower   choice="optional"  number="multiple" />
      <changed     choice="mandatory" number="multiple" />
      <source      choice="mandatory" number="single"   />
      <delete   choice="optional"  number="single"   pseudo="yes"/>
      <override choice="optional"  number="single"   pseudo="yes"/>
    </attributes>
    <dbase_code value="14"/>
    <rollback-tables>
      <table name="mntner"/>
      <table name="person_role"/>
      <table name="names"/>
      <table name="organisation"/>
    </rollback-tables>
